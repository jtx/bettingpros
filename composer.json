{
  "name": "jtx/bettingpros",
  "type": "project",
  "description": "JSON Parser",
  "license": "MIT",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/",
      "Services\\": "Services/",
      "CustomExceptions\\": "CustomExceptions/",
      "Enums\\": "Enums/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}